{
  "mcpServers": {
    "weather": {
      "command": "python",
      "args": [
        "mcp_servers/weather_server.py"
      ],
      "transport": "stdio"
    },
    "simple_retriever": {
      "command": "python",
      "args": [
        "mcp_servers/retriever_server.py"
      ],
      "transport": "stdio"
    },
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest"
      ],
      "transport": "stdio"
    }
  }
}